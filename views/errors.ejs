<% layout('/layouts/boilerplate.ejs') %>
<div class="row">
  <div class="col-6 offset-3">
    <div class="alert alert-danger" role="alert">
      <h4 class="alert-heading"><%= error.message %></h4>
      <p>
        <% if (process.env.NODE_ENV !== "production") { %>
          <p><%= error.stack %></p> 
        <% } %>        
      </p>
    </div>
  </div>
</div>
